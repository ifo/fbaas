category:            Web
build-type:          Simple
-- extra-source-files:  
cabal-version:       >=1.10

executable fbaas
  main-is:             Main.hs
  -- other-modules:       
  other-extensions:    OverloadedStrings
  build-depends:       base >=4.7 && <4.8
                      ,Spock >=0.7.7
                      ,aeson >=0.8 && <0.9
                      ,text >=1.2 && <1.3
  hs-source-dirs:      src
  default-language:    Haskell2010
